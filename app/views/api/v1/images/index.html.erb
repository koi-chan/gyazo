<h1>Api V1 Images</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @images.each do |image| %>
      <tr>
        <td><%= link_to('Show', api_v1_image_path(image)) %></td>
        <td><%= link_to('Edit', edit_api_v1_image_path(image)) %></td>
        <td><%= link_to('Destroy', api_v1_image_path(image), method: :delete, data: { confirm: 'Are you sure?' }) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api V1 Image', new_api_v1_image_path %>
